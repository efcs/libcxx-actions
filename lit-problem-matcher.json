{
  "problemMatcher": [
    {
      "owner": "lit-problem-matcher",
      "pattern": [
        {
          "regexp": "^[*]{20} TEST '(.*) :: ([^']*)' (\\w*) [*]{20}",
          "file": 2
          "code": 3,
          "fromPath": 1
        },
        {
          "regexp": "^(?!\\*{20}).*$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
